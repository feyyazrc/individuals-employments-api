{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Individual's employments",
  "type": "object",
  "properties": {
    "_links": {
      "type": "object",
      "properties": {
        "paye": {
          "type": "object",
          "description": "Refers to the next logical resource",
          "properties": {
            "href": {
              "type": "string",
              "example": "/individuals/employments/paye/match/8f9e634e-85f1-4e39-8735-609d149513d7{?fromDate,toDate}"
            },
            "title": {
              "type": "string",
              "example": "View individual's employments"
            }
          },
          "required": ["href"]
        },
        "self": {
          "type": "object",
          "description": "HAL compliant. Refers to itself",
          "properties": {
            "href": {
              "type": "string",
              "example": "/individuals/employments/match/8f9e634e-85f1-4e39-8735-609d149513d7"
            }
          },
          "required": ["href"]
        }
      },
      "required": ["paye", "self"]
    }
  },
  "required": ["_links"]
}
