{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Individual's PAYE employments",
  "type": "object",
  "properties": {
    "_links": {
      "type": "object",
      "description": "A list of links for possible actions related to this endpoint.",
      "properties": {
        "self": {
          "type": "object",
          "description": "The Uniform Resource Identifier (URI) for this endpoint.",
          "properties": {
            "href": {
              "type": "string",
              "description": "The URL of the endpoint.",
              "example": "/individuals/employments/paye?matchId=57072660-1df9-4aeb-b4ea-cd2d7f96e430&fromDate=2016-02-28&toDate=2017-03-01"
            }
          },
          "required": ["href"]
        }
      },
      "required": [
        "self"
      ]
    },
    "employments": {
      "type": "array",
      "description": "The list of the individualâ€™s PAYE employment data for the requested time period.",
      "items": {
        "type": "object",
        "properties": {
          "payFrequency": {
            "type": "string",
            "description": "How often the employee is paid.",
            "enum": [
              "WEEKLY",
              "FORTNIGHTLY",
              "FOUR_WEEKLY",
              "CALENDAR_MONTHLY",
              "QUARTERLY",
              "BI_ANNUALLY",
              "ANNUALLY",
              "ONE_OFF",
              "IRREGULAR"
            ]
          },
          "startDate": {
            "type": "string",
            "description": "The date the employment started. Date in the format: YYYY-MM-DD.",
            "example": "2019-01-01"
          },
          "endDate": {
            "type": "string",
            "description": "The date the employment ended. Date in the format: YYYY-MM-DD.",
            "example": "2019-03-01"
          },
          "employer": {
            "type": "object",
            "description": "The employer's details.",
            "properties": {
              "payeReference": {
                "type": "string",
                "description": "Employer PAYE Reference",
                "example": "123/AI45678"
              },
              "name": {
                "type": "string",
                "description": "The name of the employer."
              },
              "address": {
                "type": "object",
                "description": "The address of the employer.",
                "properties": {
                  "line1": {
                    "type": "string",
                    "description": "Address line 1."
                  },
                  "line2": {
                    "type": "string",
                    "description": "Address line 2."
                  },
                  "line3": {
                    "type": "string",
                    "description": "Address line 3."
                  },
                  "line4": {
                    "type": "string",
                    "description": "Address line 4."
                  },
                  "line5": {
                    "type": "string",
                    "description": "Address line 5."
                  },
                  "postcode": {
                    "type": "string",
                    "description": "The postcode for the address.",
                    "example": "123/AI45678"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "required": ["_links", "employments"]
}
